<h1>Register New Application</h1>

<div class='row'>
    <div class='col-md-4'>
        <%= form_for @registered_application do |f| %>
            <div class='form-group'>
                <%= f.label :name %>
                <%=f.text_field :name, class: 'form-control' %>
            </div>
            <div class='form-group'>
                <%= f.label :url %>
                <%= f.text_field :url, class: 'form-control' %>
            </div>
            <div class='form-group'>
                <%= f.submit "Save" %>
            </div>
        <% end %>
    </div>
</div>